<span id="package_buy_load"></span>
<script>
var buy_set2 = 0;

function package_buy_checkbox() {
	var coin = 0;
	var cnt = 0;
	$(".checkbox_data:checked").each(function() {
		coin += parseInt($(this).attr('data'));
		cnt += 1;
	});
	var text = "共{value}話數，消耗{value2}點券。";
	text = text.replace("{value}", cnt);
	if(50 <= cnt) {
		text = text.replace("{value2}",(coin - Math.round(coin * (50 / 100))));
	} else {
		text = text.replace("{value2}", coin);
	}
	$('#package_text').html(text);
}

function package_buy_proc() {

	$('#package_menu').hide();
	$('#package_loading').show();

	var idx = [];
	$(".checkbox_data:checked").each(function() {
		idx.push(parseInt($(this).attr('value')));
	});

	if(buy_set2 == 0 && idx.length > 0) {
		buy_set2 = 1;
		$.post("/comic/episode_package_buy_proc",
		{
			ci_token: '2696ed848d2b01a5185cc6dbf7bb1ab5',
			episode_idx: idx,
		},
		function(data, status) {
			buy_set2 = 0;
			if(data == 'NEED_LOGIN') {
				load_alert('login'); return false;
			}

			$('#package_buy_load').html(data);

		}).error(function(xhr, desc, err) { 
			error_proc(err);
		});
	} else if(idx.length == 0) {
		alert("尚未選擇要購買的話數。");
	}

}


var check_cnt = [];
$(".checkbox_data").each(function() {
	check_cnt.push(parseInt($(this).attr('value')));
});
if(check_cnt.length > 5) {
	$('#package_btn').show();
}
if(check_cnt.length >= 50) {
	$('#package_sale_info').show();
}
</script>
<div style="background-color:#fff;opacity:0.95;width:100%;bottom:0px;height:100px;left:0px;position:fixed;border-top:2px solid #ce3635;display:none;z-index:1000;" id="package_board" align="center">
	<div style="width:1120px;height:100px;left:0px;position:relative;padding:20px 0px 0px 0px;line-height:25px;text-align:left;">
		<span style="font-size:18px;font-weight:bold;color:#ce3635;">要一次購買所選之作品嗎?</span>
				&nbsp;<span style='background-color:#ce3635;color:#fff;border-radius:5px;padding:5px 10px;'>購買50回次以上， 即可獲得50%折扣優惠!</span>
				
		<br>
		<span style="font-size:15px;font-weight:bold;" id="package_text">共{value}話數，消耗{value2}點券。</span>
	
		<div style="width:300px;height:100px;top:15px;right:0px;position:absolute;padding:20px 0px 0px 0px;line-height:25px;text-align:right;" id="package_menu">
			<span style="border:1px solid #ce3635;padding:10px;border-radius:20px;font-size:16px;font-weight:bold;color:#ce3635;cursor:pointer;line-height:20px;" onclick="package_buy_checkbox();$('#package_board').hide();$('.checkbox_list').hide();$('.vote_list').show();">&nbsp;取消&nbsp;</span>	
			<span style="border:1px solid #ce3635;padding:10px;border-radius:20px;font-size:16px;font-weight:bold;color:#fff;background-color:#ce3635;cursor:pointer;line-height:20px;"
			onclick="package_buy_proc();">&nbsp;一次購買&nbsp;</span>
		</div>
		<div style="width:300px;height:100px;top:15px;right:0px;position:absolute;padding:20px 0px 0px 0px;line-height:25px;text-align:right;display:none;" id="package_loading">
			<span style="border:1px solid #ce3635;padding:10px;border-radius:20px;font-size:16px;font-weight:bold;color:#ce3635;line-height:20px;" >&nbsp;購買中&nbsp;</span>
		</div>
	</div>
</div>

