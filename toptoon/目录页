			<script>
				if (typeof jQuery == 'undefined') {
					console.log("Jquery Load ERROR");
					var temp = location.href.split("/page/");
					if(temp[1] == undefined) {
						var temp = location.href.split("/comic/");
						if(temp[1] == undefined) {
							location = "/";
						} else {
							var temp2 = temp[1].split("?comic_id=");
							if(temp2[0] == undefined || temp2[0] == '') {
								location = "/";
							} else {
								location = "/#contents_" + temp2[1];
							}
						}
					} else {
						var temp2 = temp[1].split("?");
						if(temp2[0] == undefined || temp2[0] == '') {
							location = "/";
						} else {
							location = "/#" + temp2[0];
						}
					}

						
				}
			</script>
			
